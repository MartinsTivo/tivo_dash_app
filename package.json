{
  "name": "tivo_dashboard_application",
  "version": "1.0.2",
  "description": "This is a TIVO Dashboard App",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron .",
    "development": "rimraf dist && webpack --watch --config ./webpack.dev.js --progress --colors",
    "production": "rimraf dist && webpack --config ./webpack.prod.js --progress --colors"
  },
  "build": {
    "appId": "com.tivodashboard.tivoapp",
    "win": {
      "target": "nsis",
      "icon": "media/TIVO.ico"
    },
    "productName": "TIVO Dashboard "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MartinsTivo/tivo_dash_app.git"
  },
  "author": "Martins Tiltins",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "css-loader": "^3.0.0",
    "electron": "^5.0.6",
    "electron-builder": "^21.0.15",
    "electron-is-dev": "^1.1.0",
    "electron-packager": "^14.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^3.0.4",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "update-electron-app": "^1.5.0",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
